    <script src=" ~/assets/dist/js/jquery-3.6.0.min.js"></script>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-sparklines/2.1.2/jquery.sparkline.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <script src="~/assets/dist/js/jquery-ui.js"></script>
    <script src="~/assets/dist/js/custom.min.js"></script>
    <script src="~/assets/dist/js/Student.js"></script>
    <script src="~/assets/dist/js/teacher.js"></script>
    <script src="~/assets/dist/js/project.js"></script>
    <script src="~/assets/dist/js/create.js"></script>




    <script>
        $('.myActionLink').prepend("<i class='fas fa-trash' style='color:red;'></i>");
    </script>
    <script>
        $(document).ready(function () {
            $('[data-toggle="tooltip"]').tooltip();
        });
    </script>
